{
  "name": "ntc-edge-internal",
  "version": "1.0.0",
  "scripts": {
    "start": "expo start --dev-client",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "mac": "yarn electron-webpack && yarn electron-builder --mac",
    "win": "yarn electron-webpack && yarn electron-builder --win",
    "web-build": "expo build:web && echo '/* /index.html 200' >./web-build/_redirects",
    "web-build2": "export NETLIFY_AUTH_TOKEN=LPojSmhezQZsTZPBEZmKhLMsEv-vLPECwfdL8oM_Ta4 && expo build:web && echo '/* /index.html 200' >./web-build/_redirects && netlify deploy --prod --dir ./web-build",
    "web-deploy": "netlify deploy --prod --dir ./web-build",
    "eject": "expo eject",
    "prettier:write": "npx prettier --write **/*.{js,jsx,ts,tsx,json} && npx prettier --write *.{js,jsx,ts,tsx,json}",
    "lint": "tsc --noEmit && eslint --ext .js,.jsx,.ts,.tsx ./"
  },
  "build": {
    "productName": "NTC-EDGE Portal",
    "appId": "portalapp.ntcedge.com",
    "copyright": "Â© NTCEDGE 2022",
    "directories": {
      "output": "dist"
    },
    "nsis": {
      "oneClick": false,
      "allowElevation": true,
      "allowToChangeInstallationDirectory": true,
      "installerIcon": "assets/electron.ico",
      "uninstallerIcon": "assets/electron.ico",
      "installerHeaderIcon": "assets/electron.ico",
      "createDesktopShortcut": true,
      "createStartMenuShortcut": true,
      "shortcutName": "NTC-EDGE Portal"
    },
    "win": {
      "icon": "assets/electron.ico",
      "requestedExecutionLevel": "requireAdministrator",
      "artifactName": "${productName} Setup ${version} ${arch}.${ext}",
      "target": [
        {
          "target": "nsis",
          "arch": [
            "x64",
            "ia32"
          ]
        }
      ]
    },
    "dmg": {
      "contents": [
        {
          "x": 410,
          "y": 150,
          "type": "link",
          "path": "/Applications"
        },
        {
          "x": 130,
          "y": 150,
          "type": "file"
        }
      ]
    },
    "mac": {
      "icon": "assets/electron.png",
      "identity": null,
      "compression": "store",
      "target": [
        {
          "target": "dmg"
        }
      ]
    },
    "linux": {
      "icon": "assets/electron.png"
    },
    "extraMetadata": {
      "main": "main.js"
    },
    "files": [
      {
        "from": "dist/main/",
        "to": "./",
        "filter": [
          "**/*"
        ]
      },
      {
        "from": "dist/renderer",
        "to": "./",
        "filter": [
          "**/*"
        ]
      },
      "package.json",
      "**/node_modules/**/*"
    ]
  },
