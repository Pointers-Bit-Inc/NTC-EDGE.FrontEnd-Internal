{
  "expo": {
    "name": "ntc-edge-internal",
    "slug": "ntc-edge-internal",
    "owner": "ntc-edge",
    "version": "1.0.2",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "cover",
      "backgroundColor": "#051e69"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "ios": {
      "googleServicesFile": "./google-services.json",
      "supportsTablet": true,
      "usesIcloudStorage": true,
      "bundleIdentifier": "edu.ph.ustp.ntc-edge-internal",
      "buildNumber": "1.0.2",
      "infoPlist": {
        "NSCameraUsageDescription": "This will be used for video call and live streaming.",
        "NSMicrophoneUsageDescription": "This will be used for video call and live streaming."
      }
    },
    "android": {
      "googleServicesFile": "./GoogleService-Info.plist",
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "ntc.edge.internal",
      "versionCode": 3
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "@react-native-firebase/app",
      [
        "expo-document-picker",
        {
          "appleTeamId": "955L72N94A"
        }
      ],
      [
        "expo-image-picker",
        {
          "photosPermission": "Allow $(PRODUCT_NAME) to access your photos"
        }
      ]
    ]
  }
}
